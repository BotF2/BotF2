  <DataTemplate DataType="{x:Type game:SitRepEntry}">
    <DockPanel MaxWidth="540"
               LastChildFill="True"
               TextBlock.TextAlignment="Left">
      <Image x:Name="DetailImage"
             DockPanel.Dock="Left"
             Margin="0,0,14,0"
             MaxWidth="270"
             MaxHeight="225"
             VerticalAlignment="Top"
             Source="{Binding Path=DetailImage, Converter={StaticResource ImageUriConverter}}" />
      <TextBlock DockPanel.Dock="Top"
                 FontFamily="{StaticResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                 FontSize="14pt"
                 Foreground="{StaticResource HeaderTextBrush}"
                 Margin="0,0,0,14"
                 TextWrapping="Wrap"
                 TextTrimming="None"
                 Text="{Binding Path=HeaderText}" />
      <ScrollViewer HorizontalScrollBarVisibility="Disabled"
                    VerticalScrollBarVisibility="Auto">
        <TextBlock FontFamily="{StaticResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                   FontSize="12pt"
                   Foreground="{StaticResource {x:Static scc:ClientResources.ControlTextForegroundBrushKey}}"
                   TextWrapping="Wrap"
                   TextTrimming="CharacterEllipsis"
                   av:BindingHelpers.Inlines="{Binding Path=DetailText, Converter={StaticResource FormattedTextConverter}}"/>
      </ScrollViewer>
    </DockPanel>
    <DataTemplate.Triggers>
      <DataTrigger Binding="{Binding Path=DetailImage}" Value="{x:Null}">
        <Setter TargetName="DetailImage" Property="Visibility" Value="Collapsed" />
      </DataTrigger>
    </DataTemplate.Triggers>
  </DataTemplate>

