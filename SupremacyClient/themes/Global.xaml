  <Style x:Key="LCARS_BorderStyle"
         TargetType="{x:Type ContentControl}"
         BasedOn="{StaticResource BaseStyle}">
    <Setter Property="Focusable"
            Value="False" />
    <Setter Property="FocusVisualStyle"
            Value="{x:Null}" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type ContentControl}">
          <Grid>
            <Grid.RowDefinitions>
              <RowDefinition Height="Auto" />
              <RowDefinition Height="*" />
              <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="Auto" />
              <ColumnDefinition Width="0.25*" />
              <ColumnDefinition Width="0.5*" />
              <ColumnDefinition Width="0.25*" />
              <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Path Grid.Column="0"
                  Grid.Row="0"
                  SnapsToDevicePixels="True"
                  Data="{StaticResource LCARS_Corner_TopLeft_Tall}"
                  Fill="{StaticResource PurpleGradient_TopLeft_BottomRight}" />
            <Path Grid.Column="4"
                  Grid.Row="0"
                  SnapsToDevicePixels="True"
                  Data="{StaticResource LCARS_Corner_TopRight_Tall}"
                  Fill="{StaticResource PurpleGradient_TopLeft_BottomRight}" />
            <Path Grid.Column="0"
                  Grid.Row="2"
                  SnapsToDevicePixels="True"
                  Data="{StaticResource LCARS_Corner_BottomLeft_Tall}"
                  Fill="{StaticResource PurpleGradient_TopLeft_BottomRight}" />
            <Path Grid.Column="4"
                  Grid.Row="2"
                  SnapsToDevicePixels="True"
                  Data="{StaticResource LCARS_Corner_BottomRight_Tall}"
                  Fill="{StaticResource PurpleGradient_TopLeft_BottomRight}" />
            <Grid Grid.Column="1"
                  Grid.Row="0"
                  Height="28"
                  VerticalAlignment="Top">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
              </Grid.ColumnDefinitions>
              <Rectangle x:Name="LeftTopEdge"
                         Grid.Column="1"
                         Margin="3,0,0,0"
                         Fill="{StaticResource OrangeGradient_Top_Bottom}" />
            </Grid>
            <!--<Rectangle x:Name="CenterTopEdge" Grid.Column="2" Grid.Row="0" Height="28" Margin="3,0,3,0" VerticalAlignment="Top" Fill="{StaticResource BlueGradient_Top_Bottom}"/>-->
            <Grid Grid.Column="2"
                  Grid.Row="0"
                  Height="28"
                  Margin="3,0,3,0"
                  VerticalAlignment="Top">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
              </Grid.ColumnDefinitions>
                
              <Rectangle x:Name="LeftCenterTopEdge"
                         Grid.Column="0"
                         Fill="{StaticResource BlueGradient_Top_Bottom}" />
                
              <Rectangle x:Name="RightCenterTopEdge"
                         Grid.Column="2"
                         Fill="{StaticResource BlueGradient_Top_Bottom}" />
                
            </Grid>
            <Rectangle x:Name="RightTopEdge"
                       Grid.Column="3"
                       Grid.Row="0"
                       Height="28"
                       Margin="0,0,3,0"
                       VerticalAlignment="Top"
                       Fill="{StaticResource OrangeGradient_Top_Bottom}" />
              
            <Rectangle x:Name="LeftEdge"
                       Grid.Column="0"
                       Grid.Row="1"
                       Width="8"
                       Margin="0,3,0,3"
                       HorizontalAlignment="Left"
                       Fill="{StaticResource OrangeGradient_Left_Right}" />
              
            <Rectangle x:Name="RightEdge"
                       Grid.Column="4"
                       Grid.Row="1"
                       Width="8"
                       Margin="0,3,0,3"
                       HorizontalAlignment="Right"
                       Fill="{StaticResource OrangeGradient_Left_Right}" />
              
            <Rectangle x:Name="LeftBottomEdge"
                       Grid.Column="1"
                       Grid.Row="2"
                       Height="28"
                       Margin="3,0,0,0"
                       VerticalAlignment="Bottom"
                       Fill="{StaticResource OrangeGradient_Top_Bottom}" />
              
            <Rectangle x:Name="CenterBottomEdge"
                       Grid.Column="2"
                       Grid.Row="2"
                       Height="28"
                       Margin="3,0,3,0"
                       VerticalAlignment="Bottom"
                       Fill="{StaticResource BlueGradient_Top_Bottom}" />
              
            <Rectangle x:Name="RightBottomEdge"
                       Grid.Column="3"
                       Grid.Row="2"
                       Height="28"
                       Margin="0,0,3,0"
                       VerticalAlignment="Bottom"
                       Fill="{StaticResource OrangeGradient_Top_Bottom}" />
              
            <ContentPresenter Grid.Column="1"
                              Grid.ColumnSpan="3"
                              Grid.Row="1"
                              Margin="7,0,7,0"
                              ClipToBounds="True" />
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

