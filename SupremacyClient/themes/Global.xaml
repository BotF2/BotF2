    <Style TargetType="{x:Type TabControl}"
         BasedOn="{StaticResource BaseStyle}">
    <Setter Property="OverridesDefaultStyle"
            Value="True" />
    <Setter Property="SnapsToDevicePixels"
            Value="True" />
    <Setter Property="KeyboardNavigation.IsTabStop"
            Value="False" />
    <Setter Property="FocusVisualStyle"
            Value="{x:Null}" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type TabControl}">
          <Grid Grid.IsSharedSizeScope="False"
                SnapsToDevicePixels="True">
            <Grid.RowDefinitions>
              <RowDefinition Height="Auto" />
              <RowDefinition Height="*" />
              <RowDefinition Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
              <ColumnDefinition Width="Auto" />
              <ColumnDefinition Width="*" />
              <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <Path Grid.Column="0"
                  Grid.Row="0"
                  SnapsToDevicePixels="True"
                  Data="{StaticResource LCARS_Corner_TopLeft_Tall}"
                  Fill="{StaticResource PurpleGradient_TopLeft_BottomRight}" />
            <Path Grid.Column="2"
                  Grid.Row="0"
                  SnapsToDevicePixels="True"
                  Data="{StaticResource LCARS_Corner_TopRight_Tall}"
                  Fill="{StaticResource PurpleGradient_TopLeft_BottomRight}" />
            <Path Grid.Column="0"
                  Grid.Row="2"
                  SnapsToDevicePixels="True"
                  Data="{StaticResource LCARS_Corner_BottomLeft_Tall}"
                  Fill="{StaticResource PurpleGradient_TopLeft_BottomRight}" />
            <Path Grid.Column="2"
                  Grid.Row="2"
                  SnapsToDevicePixels="True"
                  Data="{StaticResource LCARS_Corner_BottomRight_Tall}"
                  Fill="{StaticResource PurpleGradient_TopLeft_BottomRight}" />
            <Rectangle x:Name="LeftEdge"
                       Grid.Column="0"
                       Grid.Row="1"
                       Width="8"
                       Margin="0,3,0,3"
                       HorizontalAlignment="Left"
                       Fill="{StaticResource OrangeGradient_Left_Right}" />
            <Rectangle x:Name="RightEdge"
                       Grid.Column="2"
                       Grid.Row="1"
                       Width="8"
                       Margin="0,3,0,3"
                       HorizontalAlignment="Right"
                       Fill="{StaticResource OrangeGradient_Left_Right}" />
            <Rectangle x:Name="BottomEdge"
                       Grid.Column="1"
                       Grid.Row="2"
                       Height="28"
                       Margin="3,0,3,0"
                       VerticalAlignment="Bottom"
                       Fill="{StaticResource BlueGradient_Top_Bottom}" />

            <Grid Grid.Column="1"
                  Grid.Row="0"
                  Height="28"
                  VerticalAlignment="Top"
                  SnapsToDevicePixels="True">
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
              </Grid.ColumnDefinitions>
              <TabPanel x:Name="HeaderPanel"
                        Grid.Column="0"
                        Margin="0"
                        IsItemsHost="True"
                        Panel.ZIndex="1"
                        KeyboardNavigation.TabIndex="1"
                        Background="Transparent"
                        SnapsToDevicePixels="True" />
              <Rectangle Grid.Column="1"
                         Margin="3,0,3,0"
                         Fill="{StaticResource OrangeGradient_Top_Bottom}" />
            </Grid>

            <ContentPresenter x:Name="PART_SelectedContentHost" Grid.Column="1"
                              Grid.Row="1"
                              ContentSource="SelectedContent" />
          </Grid>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

