  <!--
    GameLabel Style
  -->
  <Style TargetType="scc:GameLabel">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="scc:GameLabel">
          <DockPanel LastChildFill="True">
            <Image x:Name="Image"
                   DockPanel.Dock="Left"
                   Margin="0,0,4,0"
                   MinWidth="16"
                   MinHeight="16"
                   MaxWidth="24"
                   MaxHeight="24"
                   Width="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=VariantSize}"
                   Height="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=VariantSize}"
                   Stretch="Uniform"
                   StretchDirection="DownOnly"
                   VerticalAlignment="Center"
                   Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=HasImage, Converter={StaticResource BooleanToVisibilityConverter}}">
              <Image.Source>
                <MultiBinding Converter="{scc:CoalesceConverter}">
                  <Binding RelativeSource="{RelativeSource TemplatedParent}"
                           Path="ImageSourceLarge" />
                  <Binding RelativeSource="{RelativeSource TemplatedParent}"
                           Path="ImageSourceSmall" />
                </MultiBinding>
              </Image.Source>
            </Image>
            <ContentPresenter x:Name="LabelPresenter"
                              RecognizesAccessKey="True"
                              Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=HasLabel, Converter={StaticResource BooleanToVisibilityConverter}}">
              <ContentPresenter.Content>
                <AccessText Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Label}"
                            HorizontalAlignment="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=HorizontalContentAlignment}"
                            VerticalAlignment="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=VerticalContentAlignment}" />
              </ContentPresenter.Content>
            </ContentPresenter>
          </DockPanel>
          <ControlTemplate.Triggers>
            <Trigger Property="VariantSize"
                     Value="Small">
              <Setter TargetName="LabelPresenter"
                      Property="Visibility"
                      Value="Collapsed" />
            </Trigger>
            <Trigger SourceName="LabelPresenter"
                     Property="IsVisible"
                     Value="False">
              <Setter TargetName="Image"
                      Property="Margin"
                      Value="0" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

  <!--
    GameButton Style
  -->

  <ControlTemplate x:Key="{x:Static scc:GameButton.NormalTemplateKey}"
                   TargetType="{x:Type scc:GameButton}">
    <Grid>
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition Width="Auto" />
      </Grid.ColumnDefinitions>
      <Border Grid.Column="0"
              Background="{StaticResource PurpleGradient_TopLeft_BottomRight}"
              CornerRadius="14,0,0,14"
              Width="21" />

      <DockPanel Grid.Column="1"
                 LastChildFill="True">
        <Image DockPanel.Dock="Left"
               MinWidth="16"
               MinHeight="16"
               MaxWidth="24"
               MaxHeight="24"
               Margin="3,0,0,0"
               Stretch="Uniform"
               StretchDirection="DownOnly"
               VerticalAlignment="Center"
               Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=(scc:GameControlBase.HasImage), Converter={StaticResource BooleanToVisibilityConverter}}">
          <Image.Source>
            <MultiBinding Converter="{scc:CoalesceConverter}">
              <Binding RelativeSource="{RelativeSource TemplatedParent}"
                       Path="ImageSourceLarge" />
              <Binding RelativeSource="{RelativeSource TemplatedParent}"
                       Path="ImageSourceSmall" />
            </MultiBinding>
          </Image.Source>
        </Image>
        <Border x:Name="ContentBorder"
                Margin="3,0,3,0"
                Padding="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Padding}"
                Background="{StaticResource BlueGradient_Left_Right}">
          <ContentPresenter RecognizesAccessKey="True">
            <ContentPresenter.Content>
              <AccessText Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Label}"
                          HorizontalAlignment="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=HorizontalContentAlignment}"
                          VerticalAlignment="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=VerticalContentAlignment}" />
            </ContentPresenter.Content>
          </ContentPresenter>
        </Border>
      </DockPanel>
      <Rectangle Grid.Column="2"
                 Stretch="Fill"
                 Fill="{StaticResource OrangeGradient_Left_Right}"
                 Margin="0,0,3,0"
                 Width="14" />
      <Border Grid.Column="3"
              Background="{StaticResource PurpleGradient_TopLeft_BottomRight}"
              CornerRadius="0,14,14,0"
              Width="21" />
    </Grid>
    <ControlTemplate.Triggers>
            <!--IsEnabled-->
            <Trigger Property="IsEnabled"
               Value="False">
        <Setter TargetName="ContentBorder"
                Property="Opacity"
                Value="0.33" />
      </Trigger>
            <!--IsMouseOver-->
            <Trigger Property="IsMouseOver"
               Value="True">
        <Setter TargetName="ContentBorder"
                Property="Background"
                Value="{StaticResource BlueHoverGradient_Left_Right}" />
      </Trigger>
            <!--IsPressed-->
            <Trigger Property="IsPressed"
               Value="True">
        <Setter TargetName="ContentBorder"
                Property="Background"
                Value="{StaticResource BlueGradient_Right_Left}" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  
  <ControlTemplate x:Key="{x:Static scc:GameButton.CheckboxTemplateKey}"
                   TargetType="{x:Type scc:GameButton}">
    <DockPanel LastChildFill="True">
      <Border x:Name="Check"
              DockPanel.Dock="Left"
              Width="14"
              Height="14"
              BorderBrush="{StaticResource BlueGradient_Top_Bottom}"
              BorderThickness="2"
              Background="Transparent"
              Margin="0"
              VerticalAlignment="Center">
        <Border x:Name="CheckMark"
                Width="6"
                Height="6"
                Visibility="Hidden"
                SnapsToDevicePixels="True"
                Background="{StaticResource OrangeGradient_Top_Bottom}" />
      </Border>
      <ContentPresenter RecognizesAccessKey="True">
        <ContentPresenter.Content>
          <AccessText Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Label}"
                      Margin="6,0,0,0"
                      VerticalAlignment="Center" />
        </ContentPresenter.Content>
      </ContentPresenter>
    </DockPanel>
    <ControlTemplate.Triggers>
      <Trigger Property="IsChecked"
               Value="True">
        <Setter TargetName="CheckMark"
                Property="Visibility"
                Value="Visible" />
      </Trigger>
      <Trigger Property="IsChecked"
               Value="{x:Null}">
        <Setter TargetName="CheckMark"
                Property="Background"
                Value="{StaticResource GrayGradient_Top_Bottom}" />
      </Trigger>
      <Trigger Property="IsMouseOver"
               Value="True">
        <Setter TargetName="Check"
                Property="BorderBrush"
                Value="{StaticResource BlueHoverGradient_Top_Bottom}" />
      </Trigger>
      <Trigger Property="IsEnabled"
               Value="False">
        <Setter TargetName="Check"
                Property="BorderBrush"
                Value="{StaticResource GrayGradient_Top_Bottom}" />
        <Setter TargetName="CheckMark"
                Property="Background"
                Value="{StaticResource GrayGradient_Top_Bottom}" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>

