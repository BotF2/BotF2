  <Style TargetType="{x:Type MenuItem}"
         BasedOn="{StaticResource BaseStyle}">
    <Setter Property="Foreground"
            Value="{StaticResource BeigeTextBrush}" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type MenuItem}">
          <Border x:Name="Border"
                  Padding="6,2,6,2">
            <Grid>
              <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"
                                  SharedSizeGroup="Icon" />
                <ColumnDefinition Width="*" />
              </Grid.ColumnDefinitions>
              <ContentPresenter x:Name="Icon"
                                Margin="6,0,6,0"
                                VerticalAlignment="Center"
                                ContentSource="Icon" />
              <Border x:Name="Check"
                      Width="14"
                      Height="14"
                      BorderBrush="{StaticResource BlueGradient_Top_Bottom}"
                      BorderThickness="2"
                      Background="Transparent"
                      Margin="0"
                      VerticalAlignment="Center"
                      Visibility="Collapsed">
                <Border x:Name="CheckMark"
                        Width="6"
                        Height="6"
                        Visibility="Hidden"
                        SnapsToDevicePixels="True"
                        Background="{StaticResource OrangeGradient_Top_Bottom}" />
              </Border>
              <ContentPresenter Name="HeaderHost"
                                Grid.Column="1"
                                ContentSource="Header"
                                RecognizesAccessKey="True"
                                Margin="6,0,0,0" />
            </Grid>
          </Border>
          <ControlTemplate.Triggers>
            <Trigger Property="IsEnabled"
                     Value="False">
              <Setter Property="Foreground"
                      Value="{StaticResource DisabledTextBrush}" />
            </Trigger>
            <Trigger Property="IsMouseOver"
                   Value="True">
              <Setter TargetName="Border"
                      Property="Background"
                      Value="{StaticResource GrayGradient_Top_Bottom}" />
            </Trigger>
            <Trigger Property="Icon"
                     Value="{x:Null}">
              <Setter TargetName="Icon"
                      Property="Visibility"
                      Value="Collapsed" />
            </Trigger>
            <Trigger Property="IsChecked"
                     Value="True">
              <Setter TargetName="CheckMark"
                      Property="Visibility"
                      Value="Visible" />
            </Trigger>
            <Trigger Property="IsCheckable"
                     Value="True">
              <Setter TargetName="Check"
                      Property="Visibility"
                      Value="Visible" />
              <Setter TargetName="Icon"
                      Property="Visibility"
                      Value="Collapsed" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>

