                              <TextBlock
                                          VerticalAlignment="Center"
                                          HorizontalAlignment="Right"
                                          FontWeight="DemiBold"
                                          Foreground="LightBlue"
                                          Text="Research: " />
                              <Grid>
                                <Grid.RowDefinitions>
                                  <RowDefinition Height="*" />
                                  <RowDefinition Height="24" />
                                  <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                       Grid.Row="1"
                                       Margin="0,4,0,4"
                                       VerticalAlignment="Center"
                                       HorizontalAlignment="Left" />
                                <scc:GameButton
                                            Name="StealResearchSix"
                                            Grid.Row="1"
                                            DisplayMode="Tiny"
                                            VerticalAlignment="Center"
                                            HorizontalAlignment="Left"
                                            Label="Steal Research"
                                            Visibility="Collapsed"
                                            FontWeight="DemiBold"
                                            Foreground="Black"
                                            Background="Transparent"
                                            BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                            BorderThickness="2"
                                            Click = "OnResearchSixClick"/>
                              </Grid>
                              <!--Credit Six-->
                              <scc:GameLabel
                                            VerticalAlignment="Center"
                                            Margin="14,0,2,0"
                                            ImageSourceSmall="vfs:///Resources/Images/ScreenIcons/credit.png" />
                              <TextBlock
                                      VerticalAlignment="Center"
                                      HorizontalAlignment="Right"
                                      FontWeight="DemiBold"
                                      Foreground="LightBlue"
                                      Text="Credits: " />
                              <Grid>
                                <Grid.RowDefinitions>
                                  <RowDefinition Height="*" />
                                  <RowDefinition Height="24" />
                                  <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                       Grid.Row="1"
                                       Margin="0,4,0,4"
                                       VerticalAlignment="Center"
                                       HorizontalAlignment="Left" />
                                <scc:GameButton
                                          Name="StealCreditsSix"
                                          Grid.Row="1"
                                          DisplayMode="Tiny"
                                          VerticalAlignment="Center"
                                          HorizontalAlignment="Left"
                                          Label="Steal Credits"
                                          Visibility="Collapsed"
                                          FontWeight="DemiBold"
                                          Foreground="Black"
                                          Background="Transparent"
                                          BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                          Click = "OnCreditsSixClick"/>
                              </Grid>
                            </StackPanel>
                            <!-- Colonies - 6 -->
                            <StackPanel Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="17" HorizontalAlignment="Left">
                              <v:SpiedSixColonyList Grid.Row="16" DataContext="{Binding Path=SpiedSixColonies}" Margin="10,0,0,0" />
                            </StackPanel>
                          </Grid>
                        </Grid>
                      </Expander>
                      <TextBlock Height="5"></TextBlock>
                    </StackPanel>
                  </ScrollViewer>
                </StackPanel>
              </TabItem>
    <!--TAB for COLONIES-->
        <TabItem Header="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=COLONIES_TAB_HEADER}">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="20"/>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <TextBlock 
                        Grid.Column="0" Text="Total Empire Population"
                        Foreground="{DynamicResource {x:Static scc:ClientResources.DefaultTextForegroundBrushKey}}"
