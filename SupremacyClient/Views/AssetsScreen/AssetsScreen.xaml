    <scc:ThemedUserControl x:Class="Supremacy.Client.Views.AssetsScreen"
                           x:Name="AssetsScreen1"
                       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                       xmlns:scc="http://schemas.startreksupremacy.com/xaml/presentation"
                       xmlns:s="http://schemas.startreksupremacy.com/xaml/core"
                       xmlns:sx="http://schemas.startreksupremacy.com/xaml/core/markup"
                       xmlns:v="clr-namespace:Supremacy.Client.Views"
                       mc:Ignorable="d"
                       d:DesignHeight="768"
                       d:DesignWidth="1280"
                       d:DataContext="{d:DesignInstance Type=scc:AssetsScreenPresentationModel, IsDesignTimeCreatable=True}"
                       Foreground="{DynamicResource {x:Static scc:ClientResources.DefaultTextForegroundBrushKey}}"
                       Background="#66000000"
                       UseLayoutRounding="True"
                       Margin="6,6,6,6">

    <UserControl.Resources>
        <Style x:Key="changeColorOnExpanded" TargetType="Expander">
            <Setter Property="Margin" Value="5,0,0,0"/>
        </Style>

    </UserControl.Resources>
    <TabControl>
        <TabItem Header="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=SPY_REPORT_TAB_HEADER}">
 <!-- Spy Tab and Grid just for Head line -->
            <StackPanel>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="25" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="0" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <!-- Header -->
                    <TextBlock 
                        Grid.Column="0" 
                        Text="  Total Empire Intelligence:"
                        Foreground="{DynamicResource {x:Static scc:ClientResources.DefaultTextForegroundBrushKey}}"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="1" 
                        Text=" attaced = "
                        HorizontalAlignment="Right"
                        Foreground="LightGoldenrodYellow"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="2"
                        Text="{Binding ElementName=AssetsScreen1, Path=AttackedMeter}"
                        HorizontalAlignment="Left"
                        Foreground="OrangeRed"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="3" Text="attacking  = "
                        HorizontalAlignment="Right"
                        Foreground="LightGoldenrodYellow"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="4"
                        Text="{Binding ElementName=AssetsScreen1, Path=AttackingMeter}"
                        HorizontalAlignment="Left"
                        Foreground="OrangeRed"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="5" Text="  Your Accumulated Defence Points = "
                        HorizontalAlignment="Right"
                        Foreground="LightGoldenrodYellow"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="6" Text="{Binding Path=TotalIntelligenceDefenseAccumulated}" 
                        HorizontalAlignment="Left"
                        Foreground="OrangeRed"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <StackPanel Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="9">
                    <Grid>     
                      <!-- Row two Grid for explanation statment and summary buttons -->
                          <Grid.RowDefinitions>
                              <RowDefinition Height="38" />
                              <RowDefinition Height="Auto"/>
                          </Grid.RowDefinitions>
                          <Grid.ColumnDefinitions>
                              <ColumnDefinition Width="Auto" />
                              <ColumnDefinition Width="Auto" />
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="*"/>
                          </Grid.ColumnDefinitions>  
                          <TextBlock 
                                Grid.Row="0" 
                                Grid.Column="0"
                                VerticalAlignment="Center"
                                Text="  To spy on a major Empire below: "
                                Foreground="{DynamicResource {x:Static scc:ClientResources.DefaultTextForegroundBrushKey}}"
                                FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                                FontSize="12pt"/> 
                          <TextBlock 
                                Grid.Row="0"
                                Grid.Column="2"
                                VerticalAlignment="Center"
                                Text="Send a Spy Ship to the Empire's system and right click the ship to order "
                                Foreground="LightGray"
                                FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                                FontSize="12pt"/>   
                          <TextBlock 
                                Grid.Row="0" 
                                Grid.Column="3"
                                VerticalAlignment="Center"
                                Text="&quot;Install a Spy Network&quot;"
                                Foreground="White"
                                FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                                FontSize="12pt"/> 
                      <!-- Summary Button -->
                          <Button x:Name="SummaryButton"
                                  Grid.Row="0"
                                  Grid.Column="4"
                                  VerticalAlignment="Center"
                                  HorizontalAlignment="Right"
                                  Width="250"
                                  Height="32"
                                  Style="{StaticResource LCARS_StandardButtonStyle}"
                                  Content="Click for Turn Summary"
                                  FontSize="14pt"
                                  Foreground="Orange"
                                  FontFamily="{StaticResource {x:Static scc:ClientResources.HeaderFontFamilyKey}}"                                  
                                  Margin="5,5,0,5"
                                  Command="{x:Static scc:ClientCommands.ShowEndOfTurnSummary}">
                         </Button>
                     </Grid>
                  </StackPanel>
              </Grid>
                <ScrollViewer 
                    VerticalScrollBarVisibility="Auto"
                    Margin="5,0,0,0"
                    Width="Auto"
                    Height="800"
                    HorizontalAlignment="left">
                    <StackPanel Height="1400" Margin="5,0,0,0">
        <!-- One Expander -->
                        <Expander x:Name="EmpireExpanderOne" Background="Transparent" FlowDirection="LeftToRight" Style="{StaticResource changeColorOnExpanded}"
                                 IsExpanded="True" Header="{Binding Path=SpiedOneCivName}" FontSize="20" BorderThickness="3" BorderBrush="White"
                                  Foreground="LightYellow" VerticalAlignment="Top" HorizontalAlignment="Left" Grid.ColumnSpan="4" Grid.Row="3">
                            <Grid Name="EmpireGridOne" FlowDirection="LeftToRight" Margin="1,1,5,5" HorizontalAlignment="Center">
                                <Grid FlowDirection="LeftToRight">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="45" />
                                        <RowDefinition Height="5" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="45" />
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>                                  
                                    <!--Blame Buttons 5 -->
                                    <Border Grid.Row="0"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="18"
                                            BorderThickness="3"
                                            Margin="10,0,0,0"
                                            Background="Black"
                                            BorderBrush="DarkOrange" />
                                    <ScrollViewer
                                              HorizontalScrollBarVisibility="Auto"
                                              VerticalScrollBarVisibility="Disabled"
                                              HorizontalAlignment="Left"
                                              Grid.Row="0"
                                              Grid.ColumnSpan="18"
                                              VerticalAlignment="Top"
                                              Margin="0,5,0,0"
                                              Width="1000">
                                        <StackPanel Name="Blame5"
                                                  Orientation="Horizontal"
                                                  VerticalAlignment="Top"
                                                  Margin="15,0,0,0"                    
                                                  Grid.Row="0"
                                                  Grid.ColumnSpan="18"
                                                  Grid.Column="0">
                                            <StackPanel Name="BlameText1">
                                                <TextBlock
                                                     VerticalAlignment="Top"
                                                     HorizontalAlignment="Left"
                                                     Margin="5,10,0,0"
                                                     Foreground="LightGoldenrodYellow"
                                                     FontSize="16"
                                                     Text="1) Blame your actions on: " />
                                            </StackPanel>
                                            <StackPanel Name="BlameTerrorists1"
                                                      Orientation="Horizontal" >
                                                <Image  Height="20"
                                                    VerticalAlignment="Top"
                                                    Source="vfs:///Resources/Images/Insignias/Sabotage.png"
                                                    Margin="5,10,0,0" >
                                                </Image>
                                                <RadioButton
                                                    Name="Terrorists1"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio1"
                                                    Foreground="White"
                                                    Content="Terrorists" 
                                                    Tag="1"
                                                    Checked="OnBlameButtonsOneClick"/>
                                            </StackPanel>
                                            <StackPanel Name="NoOneButton1">
                                                <RadioButton
                                                    VerticalAlignment="Top"
                                                    Name="BlameNoOne1"
                                                    Margin="5,10,0,0"
                                                    GroupName="BlameRadio1"
                                                    Foreground="White"
                                                    Content="Blame No One" 
                                                    Tag="0"
                                                    Checked="OnBlameButtonsOneClick"/>
                                            </StackPanel>
                                            <StackPanel Name="BlameFederation1"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image Height="20"
                                                    VerticalAlignment="Top"
                                                    Source="vfs:///Resources/Images/Insignias/Federation.png"
                                                    Margin="10,10,5,0" >
                                                </Image>
                                                <RadioButton
                                                    Name="Federation1"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio1"
                                                    Foreground="LightBlue"
                                                    Content="Federation"
                                                    Tag="2"
                                                    Checked="OnBlameButtonsOneClick"/>
                                            </StackPanel>
                                            <StackPanel Name="BlameTerranEmpire1"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                    VerticalAlignment="Top"
                                                    Height="20"
                                                    Source="vfs:///Resources/Images/Insignias/TerranEmpire.png"
                                                    Margin="10,10,0,0" >
                                                </Image>
                                                <RadioButton
                                                    Name="TerranEmpire1"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio1"
                                                    Foreground="LightYellow"
                                                    Content="Terran Empire"
                                                    Tag="3"
                                                    Checked="OnBlameButtonsOneClick"/>
                                            </StackPanel>
                                            <StackPanel Name="BlameRomulans1"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                    VerticalAlignment="Top"
                                                    Height="20"
                                                    Source="vfs:///Resources/Images/Insignias/Romulans.png"
                                                    Margin="10,10,0,0" >
                                                </Image>
                                                <RadioButton
                                                    Name="Romulans1"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio1"
                                                    Foreground="LightGreen"
                                                    Content="Romulans"
                                                    Tag="4"
                                                    Checked="OnBlameButtonsOneClick"/>
                                            </StackPanel>
                                            <StackPanel Name="BlameKlingons1"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                    VerticalAlignment="Top"
                                                    Height="20"
                                                    Source="vfs:///Resources/Images/Insignias/Klingons.png"
                                                    Margin="10,10,0,0" >
                                                </Image>
                                                <RadioButton
                                                    Name="Klingons1"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio1"
                                                    Foreground="Red"
                                                    Content="Klingons"
                                                    Tag="5"
                                                    Checked="OnBlameButtonsOneClick"/>
                                            </StackPanel>
                                            <StackPanel Name="BlameCardassians1"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                    VerticalAlignment="Top"
                                                    Height="20"
                                                    Source="vfs:///Resources/Images/Insignias/Cardassians.png"
                                                    Margin="10,10,0,0" >
                                                </Image>
                                                <RadioButton
                                                    Name="Cardassians1"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio1"
                                                    Foreground="Goldenrod"
                                                    Content="Cardassianss"
                                                    Tag="6"
                                                    Checked="OnBlameButtonsOneClick"/>
                                            </StackPanel>
                                            <StackPanel Name="BlameDominion1"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                      VerticalAlignment="Top"
                                                      Height="20"
                                                      Source="vfs:///Resources/Images/Insignias/Dominion.png"
                                                      Margin="10,10,0,0" >
                                                </Image>
                                                <RadioButton
                                                      Name="Dominion1"
                                                      VerticalAlignment="Top"
                                                      Margin="0,10,0,0"
                                                      GroupName="BlameRadio1"
                                                      Foreground="MediumPurple"
                                                      Content="Dominion"
                                                      Tag="7"
                                                      Checked="OnBlameButtonsOneClick"/>
                                            </StackPanel>
                                            <StackPanel Name="BlameBorg1"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                      VerticalAlignment="Top"
                                                      Height="20"
                                                      Source="vfs:///Resources/Images/Insignias/Borg.png"
                                                      Margin="10,10,0,0" >
                                                </Image>
                                                <RadioButton
                                                      Name="Borg1"
                                                      VerticalAlignment="Top"
                                                      Margin="0,10,0,0"
                                                      GroupName="BlameRadio1"
                                                      Foreground="YellowGreen"
                                                      Content="Borg"
                                                      Tag="8"
                                                      Checked="OnBlameButtonsOneClick"/>
                                            </StackPanel>
                                        </StackPanel>
                                    </ScrollViewer>
                                  <Border Grid.Row="3"
                                          Grid.Column="0"
                                          Grid.ColumnSpan="16"
                                          BorderThickness="2"
                                          Margin="10,0,0,5"
                                          Background="Black"
                                          BorderBrush="Red" />
                                  <!-- Covert Ops buttons One -->
                                  <StackPanel Grid.Row="3"
                                            Grid.Column="0"
                                            Orientation="Horizontal"
                                            HorizontalAlignment="Left"
                                            Margin="10,0,0,0">
                                    <TextBlock
                                         VerticalAlignment="Center"
                                         HorizontalAlignment="Left"
                                         Margin="10,0,0,0"
                                         FontSize="16"
                                         Foreground="AntiqueWhite"
                                         Text="2) Target: " />
                                    <TextBlock VerticalAlignment="Center"
                                               Text="{Binding Path=SpiedOneCivName}"/>
                                    <Image Name="InsigniaOne" Margin="5,0,0,0">
                                    </Image>
                                    <!--Food One-->
                                    <scc:GameLabel
                                                   VerticalAlignment="Center"
                                                   Margin="14,0,2,0"
                                                   ImageSourceSmall="vfs:///Resources/Images/Resources/food.png"
                                                   ToolTip="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=FOOD_TOOL_TIP}" />
                                    <TextBlock
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Right"
                                                FontWeight="DemiBold"
                                                Foreground="LightGreen"
                                                Text="Food: " />
                                    <Grid>
                                      <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="24" />
                                        <RowDefinition Height="*" />
                                      </Grid.RowDefinitions>
                                      <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                             Grid.Row="1"
                                             Margin="0,4,0,4"
                                             VerticalAlignment="Center"
                                             HorizontalAlignment="Left" />
                                      <scc:GameButton Name="SabotageFoodOne"
                                                      Grid.Row="1"
                                                      DisplayMode="Tiny"
                                                      VerticalAlignment="Center"
                                                      HorizontalAlignment="Left"
                                                      FontWeight="DemiBold"
                                                      Label="Sabotage Food"
                                                      Visibility="Collapsed"
                                                      Foreground="Black"
                                                      Background="Transparent"
                                                      BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                                      BorderThickness="2"
                                                      Click = "OnFoodOneClick"/>
                                    </Grid>
                                    <!--Industry One-->
                                    <scc:GameLabel
                                                    VerticalAlignment="Center"
                                                    Margin="14,0,2,0"
                                                    ImageSourceSmall="vfs:///Resources/Images/ScreenIcons/industry.png"
                                                    ToolTip="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=INDUSTRY_TOOL_TIP}" />
                                    <TextBlock
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Right"
                                                FontWeight="DemiBold"
                                                Foreground="Orange"
                                                Text="Industry: " />
                                    <Grid>
                                      <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="24" />
                                        <RowDefinition Height="*" />
                                      </Grid.RowDefinitions>
                                      <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                             Grid.Row="1"
                                             Margin="0,4,0,4"
                                             VerticalAlignment="Center"
                                             HorizontalAlignment="Left" />
                                      <scc:GameButton
                                                  Name="SabotageIndustryOne"
                                                  Grid.Row="1"
                                                  DisplayMode="Tiny"
                                                  VerticalAlignment="Center"
                                                  HorizontalAlignment="Left"
                                                  Label="Sabotage Industry"
                                                  Visibility="Collapsed"
                                                  FontWeight="DemiBold"
                                                  Foreground="Black"
                                                  Background="Transparent"
                                                  BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                                  Click = "OnIndustryOneClick" />
                                    </Grid>
                                    <!--Energy One-->
                                    <scc:GameLabel
                                                    VerticalAlignment="Center"
                                                    Margin="14,0,2,0"
                                                    ImageSourceSmall="vfs:///Resources/Images/ScreenIcons/energy.png"
                                                    ToolTip="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=ENERGY_TOOL_TIP}" />
                                    <TextBlock
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Right"
                                                FontWeight="DemiBold"
                                                Foreground="Yellow"
                                                Text="Energy: " />
                                    <Grid>
                                      <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="24" />
                                        <RowDefinition Height="*" />
                                      </Grid.RowDefinitions>
                                      <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                             Grid.Row="1"
                                             Margin="0,4,0,4"
                                             VerticalAlignment="Center"
                                             HorizontalAlignment="Left" />
                                      <scc:GameButton
                                                  Name="SabotageEnergyOne"
                                                  Grid.Row="1"
                                                  DisplayMode="Tiny"
                                                  VerticalAlignment="Center"
                                                  HorizontalAlignment="Left"
                                                  Label="Sabotage Energy"
                                                  Visibility="Collapsed"
                                                  FontWeight="DemiBold"
                                                  Foreground="Black"
                                                  Background="Transparent"
                                                  BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                                  Margin="0,0,5,0"
                                                  Click = "OnEnergyOneClick" />
                                    </Grid>
                                    <!--Research One-->
                                    <scc:GameLabel
                                               VerticalAlignment="Center"
                                               Margin="14,0,2,0"
                                               ImageSourceSmall="vfs:///Resources/Images/ScreenIcons/research.png"
                                               ToolTip="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=RESEARCH_TOOL_TIP}" />
                                    <TextBlock
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Right"
                                                FontWeight="DemiBold"
                                                Foreground="LightBlue"
                                                Text="Research: " />
                                    <Grid>
                                      <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="24" />
                                        <RowDefinition Height="*" />
                                      </Grid.RowDefinitions>
                                      <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                             Grid.Row="1"
                                             Margin="0,4,0,4"
                                             VerticalAlignment="Center"
                                             HorizontalAlignment="Left" />
                                      <scc:GameButton
                                                  Name="StealResearchOne"
                                                  Grid.Row="1"
                                                  DisplayMode="Tiny"
                                                  VerticalAlignment="Center"
                                                  HorizontalAlignment="Left"
                                                  Label="Steal Research"
                                                  Visibility="Collapsed"
                                                  FontWeight="DemiBold"
                                                  Foreground="Black"
                                                  Background="Transparent"
                                                  BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                                  BorderThickness="2"
                                                  Click = "OnResearchOneClick"/>
                                    </Grid>
                                    <!--Credit One-->
                                    <scc:GameLabel
                                                  VerticalAlignment="Center"
                                                  Margin="14,0,2,0"
                                                  ImageSourceSmall="vfs:///Resources/Images/ScreenIcons/credit.png" />
                                    <TextBlock
                                            VerticalAlignment="Center"
                                            HorizontalAlignment="Right"
                                            FontWeight="DemiBold"
                                            Foreground="LightBlue"
                                            Text="Credits: " />
                                    <Grid>
                                      <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="24" />
                                        <RowDefinition Height="*" />
                                      </Grid.RowDefinitions>
                                      <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                             Grid.Row="1"
                                             Margin="0,4,0,4"
                                             VerticalAlignment="Center"
                                             HorizontalAlignment="Left" />
                                      <scc:GameButton
                                                Name="StealCreditsOne"
                                                Grid.Row="1"
                                                DisplayMode="Tiny"
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Left"
                                                Label="Steal Credits"
                                                Visibility="Collapsed"
                                                FontWeight="DemiBold"
                                                Foreground="Black"
                                                Background="Transparent"
                                                BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                                Click = "OnCreditsOneClick"/>
                                    </Grid>
                                  </StackPanel>                                
                                    <!-- Colonies - One -->
                                    <StackPanel Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="17" HorizontalAlignment="Left">
                                        <v:SpiedOneColonyList Grid.Row="16" DataContext="{Binding Path=SpiedOneColonies}" Margin="10,0,0,0" />
                                    </StackPanel>
                                </Grid>
                            </Grid>                            
                        </Expander>
                        <TextBlock Height="5"></TextBlock>
                      <!-- Two Expander -->
                      <Expander x:Name="EmpireExpanderTwo" Background="Transparent" FlowDirection="LeftToRight" Style="{StaticResource changeColorOnExpanded}"
                               IsExpanded="True" Header="{Binding Path=SpiedTwoCivName}" FontSize="20" BorderThickness="3" BorderBrush="White" Foreground="LightYellow"
                                VerticalAlignment="Top" HorizontalAlignment="Left" Grid.ColumnSpan="4" Grid.Row="3">
                        <Grid Name="EmpireGridTwo" FlowDirection="LeftToRight" Margin="1,1,5,5" HorizontalAlignment="Center">
                          <Grid FlowDirection="LeftToRight">
                            <Grid.RowDefinitions>
                              <RowDefinition Height="45" />
                              <RowDefinition Height="5" />
                              <RowDefinition Height="Auto" />
                              <RowDefinition Height="45" />
                              <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                              <ColumnDefinition Width="*" />
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                              <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <!--Blame Buttons 2 -->
                            <Border Grid.Row="0"
                                    Grid.Column="0"

                                    Grid.ColumnSpan="18"
                                    BorderThickness="3"
                                    Margin="10,0,0,0"
                                    Background="Black"
                                    BorderBrush="DarkOrange" />
                            <ScrollViewer
                                      HorizontalScrollBarVisibility="Auto"
                                      VerticalScrollBarVisibility="Disabled"
                                      HorizontalAlignment="Left"
                                      Grid.Row="0"
                                      Grid.ColumnSpan="18"
                                      VerticalAlignment="Top"
                                      Margin="0,5,0,0"
                                      Width="1000">
                              <StackPanel Name="Blame2"
                                        Orientation="Horizontal"
                                        VerticalAlignment="Top"
                                        Margin="15,0,0,0"
                                        Grid.Row="0"
                                        Grid.ColumnSpan="18"
                                        Grid.Column="0">
                                <StackPanel Name="BlameText2">
