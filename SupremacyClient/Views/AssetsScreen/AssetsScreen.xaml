<scc:ThemedUserControl x:Class="Supremacy.Client.Views.AssetsScreen"
                       x:Name="AssetsScreen1"
                       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                       xmlns:scc="http://schemas.startreksupremacy.com/xaml/presentation"
                       xmlns:s="http://schemas.startreksupremacy.com/xaml/core"
                       xmlns:sx="http://schemas.startreksupremacy.com/xaml/core/markup"
                       xmlns:v="clr-namespace:Supremacy.Client.Views"
                       mc:Ignorable="d"
                       d:DesignHeight="768"
                       d:DesignWidth="1280"
                       d:DataContext="{d:DesignInstance Type=scc:AssetsScreenPresentationModel, IsDesignTimeCreatable=True}"
                       Foreground="{DynamicResource {x:Static scc:ClientResources.DefaultTextForegroundBrushKey}}"
                       Background="#66000000"
                       UseLayoutRounding="True"
                       Margin="6,6,6,6">

    <UserControl.Resources>
        <Style x:Key="changeColorOnExpanded" TargetType="Expander">
            <Setter Property="Margin" Value="5,0,0,0"/>
        </Style>

    </UserControl.Resources>
    <TabControl>
        <TabItem Header="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=SPY_REPORT_TAB_HEADER}">
            <!-- Spy Tab and Grid just for Head line -->
            <StackPanel>
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="25" />
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="0" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>
                    <!-- Header -->
                    <TextBlock 
                        Grid.Column="0" 
                        Text="  Intelligence Resources:"
                        Foreground="{DynamicResource {x:Static scc:ClientResources.DefaultTextForegroundBrushKey}}"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="16pt"/>
                    <TextBlock 
                        Grid.Column="1" 
                        Text="Your Intelligence Points Per Turn = "
                        HorizontalAlignment="Right"
                        Foreground="LightGoldenrodYellow"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="2"
                        Text="{Binding Path=TotalIntelligenceProduction, Mode=OneWay}"
                        HorizontalAlignment="Left"
                        Foreground="OrangeRed"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="3"
                        Text="Accumulated Attack Points = "
                        HorizontalAlignment="Right"
                        Foreground="LightGoldenrodYellow"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="4"
                        Text="{Binding Path=TotalIntelligenceAttackingAccumulated, Mode=OneWay}"
                        HorizontalAlignment="Left"
                        Foreground="OrangeRed"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="5"
                        Text="Accumulated Defence Points = "
                        HorizontalAlignment="Right"
                        Foreground="LightGoldenrodYellow"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <TextBlock 
                        Grid.Column="6"
                        Text="{Binding Path=TotalIntelligenceDefenseAccumulated, Mode=OneWay}" 
                        HorizontalAlignment="Left"
                        Foreground="OrangeRed"
                        FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                        FontSize="14pt"/>
                    <StackPanel Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="9">
                        <Grid>
                            <!-- Row two Grid for explanation statment and summary buttons -->
                            <Grid.RowDefinitions>
                                <RowDefinition Height="38" />
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock 
                                Grid.Row="0" 
                                Grid.Column="0"
                                VerticalAlignment="Center"
                                Text="  To spy on a major Empire below: "
                                Foreground="{DynamicResource {x:Static scc:ClientResources.DefaultTextForegroundBrushKey}}"
                                FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                                FontSize="12pt"/>
                            <TextBlock 
                                Grid.Row="0"
                                Grid.Column="2"
                                VerticalAlignment="Center"
                                Text="Send a Spy Ship to the Empire's system and right click the ship to order "
                                Foreground="LightGray"
                                FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                                FontSize="12pt"/>
                            <TextBlock 
                                Grid.Row="0" 
                                Grid.Column="3"
                                VerticalAlignment="Center"
                                Text="&quot;Install a Spy Network&quot;"
                                Foreground="White"
                                FontFamily="{DynamicResource {x:Static scc:ClientResources.DefaultFontFamilyKey}}"
                                FontSize="12pt"/>
                            <!-- Summary Button -->
                            <Button x:Name="SummaryButton"
                                  Grid.Row="0"
                                  Grid.Column="4"
                                  VerticalAlignment="Center"
                                  HorizontalAlignment="Right"
                                  Width="250"
                                  Height="32"
                                  Style="{StaticResource LCARS_StandardButtonStyle}"
                                  Content="Click for Turn Summary"
                                  FontSize="14pt"
                                  Foreground="Orange"
                                  FontFamily="{StaticResource {x:Static scc:ClientResources.HeaderFontFamilyKey}}"                                  
                                  Margin="5,5,0,5"
                                  Command="{x:Static scc:ClientCommands.ShowEndOfTurnSummary}">
                            </Button>
                        </Grid>
                    </StackPanel>
                </Grid>
                <ScrollViewer 
                    VerticalScrollBarVisibility="Auto"
                    Margin="5,0,0,0"
                    Width="Auto"
                    Height="800"
                    HorizontalAlignment="left">
                    <StackPanel Height="1400" Margin="5,0,0,0">
      <!-- Zero Expander -->
                        <Expander x:Name="EmpireExpanderZero" Background="Transparent" FlowDirection="LeftToRight" Style="{StaticResource changeColorOnExpanded}"
                                 IsExpanded="True" Header="{Binding Path=SpiedFED}" FontSize="20" BorderThickness="3" BorderBrush="White"
                                  Foreground="LightYellow" VerticalAlignment="Top" HorizontalAlignment="Left" Grid.ColumnSpan="4" Grid.Row="3">
                            <Grid Name="EmpireGridZero" FlowDirection="LeftToRight" Margin="1,1,5,5" HorizontalAlignment="Center">
                                <Grid FlowDirection="LeftToRight">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="45" />
                                        <RowDefinition Height="5" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="45" />
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <!--Blame Buttons Zero -->
                                    <Border Grid.Row="0"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="18"
                                            BorderThickness="3"
                                            Margin="10,0,0,0"
                                            Background="Black"
                                            BorderBrush="DarkOrange" />
                                    <ScrollViewer
                                              HorizontalScrollBarVisibility="Auto"
                                              VerticalScrollBarVisibility="Disabled"
                                              HorizontalAlignment="Left"
                                              Grid.Row="0"
                                              Grid.ColumnSpan="18"
                                              VerticalAlignment="Top"
                                              Margin="0,5,0,0"
                                              Width="1000">
                                        <StackPanel x:Name="Blame0"
                                                  Orientation="Horizontal"
                                                  VerticalAlignment="Top"
                                                  Margin="15,0,0,0"                    
                                                  Grid.Row="0"
                                                  Grid.ColumnSpan="18"
                                                  Grid.Column="0">
                                            <StackPanel x:Name="BlameText0">
                                                <TextBlock
                                                     VerticalAlignment="Top"
                                                     HorizontalAlignment="Left"
                                                     Margin="5,10,0,0"
                                                     Foreground="LightGoldenrodYellow"
                                                     FontSize="16"
                                                     Text="1) Blame your actions on: " />
                                            </StackPanel>
                                            <StackPanel x:Name="BlameTerrorists0"
                                                      Orientation="Horizontal" >
                                                <Image  Height="20"
                                                    VerticalAlignment="Top"
                                                    Source="vfs:///Resources/Images/Insignias/Sabotage.png"
                                                    Margin="5,10,0,0" />
                                                <RadioButton
                                                    Name="Terrorists0"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio0"
                                                    Foreground="White"
                                                    Content="Terrorists" 
                                                    Tag="1"
                                                    Checked="OnBlameButtonsZeroClick"/>
                                            </StackPanel>
                                            <StackPanel x:Name="NoOneButton0">
                                                <RadioButton
                                                    VerticalAlignment="Top"
                                                    x:Name="BlameNoOne0"
                                                    Margin="5,10,0,0"
                                                    GroupName="BlameRadio0"
                                                    Foreground="White"
                                                    Content="Blame No One" 
                                                    Tag="0"
                                                    Checked="OnBlameButtonsZeroClick"/>
                                            </StackPanel>
                                            <StackPanel x:Name="BlameFederation0"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image Height="20"
                                                    VerticalAlignment="Top"
                                                    Source="vfs:///Resources/Images/Insignias/Federation.png"
                                                    Margin="10,10,5,0" />
                                                <RadioButton
                                                    x:Name="Federation0"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio0"
                                                    Foreground="LightBlue"
                                                    Content="Federation"
                                                    Tag="2"
                                                    Checked="OnBlameButtonsZeroClick"/>
                                            </StackPanel>
                                            <StackPanel x:Name="BlameTerranEmpire0"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                    VerticalAlignment="Top"
                                                    Height="20"
                                                    Source="vfs:///Resources/Images/Insignias/TerranEmpire.png"
                                                    Margin="10,10,0,0" />
                                                <RadioButton
                                                    x:Name="TerranEmpire0"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio0"
                                                    Foreground="LightYellow"
                                                    Content="Terran Empire"
                                                    Tag="3"
                                                    Checked="OnBlameButtonsZeroClick"/>
                                            </StackPanel>
                                            <StackPanel x:Name="BlameRomulans0"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                    VerticalAlignment="Top"
                                                    Height="20"
                                                    Source="vfs:///Resources/Images/Insignias/Romulans.png"
                                                    Margin="10,10,0,0" />
                                                <RadioButton
                                                    x:Name="Romulans0"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio0"
                                                    Foreground="LightGreen"
                                                    Content="Romulans"
                                                    Tag="4"
                                                    Checked="OnBlameButtonsZeroClick"/>
                                            </StackPanel>
                                            <StackPanel x:Name="BlameKlingons0"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                    VerticalAlignment="Top"
                                                    Height="20"
                                                    Source="vfs:///Resources/Images/Insignias/Klingons.png"
                                                    Margin="10,10,0,0" />
                                                <RadioButton
                                                    x:Name="Klingons0"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio0"
                                                    Foreground="Red"
                                                    Content="Klingons"
                                                    Tag="5"
                                                    Checked="OnBlameButtonsZeroClick"/>
                                            </StackPanel>
                                            <StackPanel x:Name="BlameCardassians0"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                    VerticalAlignment="Top"
                                                    Height="20"
                                                    Source="vfs:///Resources/Images/Insignias/Cardassians.png"
                                                    Margin="10,10,0,0" />
                                                <RadioButton
                                                    x:Name="Cardassians0"
                                                    VerticalAlignment="Top"
                                                    Margin="0,10,0,0"
                                                    GroupName="BlameRadio0"
                                                    Foreground="Goldenrod"
                                                    Content="Cardassianss"
                                                    Tag="6"
                                                    Checked="OnBlameButtonsZeroClick"/>
                                            </StackPanel>
                                            <StackPanel x:Name="BlameDominion0"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                      VerticalAlignment="Top"
                                                      Height="20"
                                                      Source="vfs:///Resources/Images/Insignias/Dominion.png"
                                                      Margin="10,10,0,0" />
                                                <RadioButton
                                                      x:Name="Dominion0"
                                                      VerticalAlignment="Top"
                                                      Margin="0,10,0,0"
                                                      GroupName="BlameRadio0"
                                                      Foreground="MediumPurple"
                                                      Content="Dominion"
                                                      Tag="7"
                                                      Checked="OnBlameButtonsZeroClick"/>
                                            </StackPanel>
                                            <StackPanel x:Name="BlameBorg0"
                                                        Visibility="Collapsed"
                                                        Orientation="Horizontal">
                                                <Image
                                                      VerticalAlignment="Top"
                                                      Height="20"
                                                      Source="vfs:///Resources/Images/Insignias/Borg.png"
                                                      Margin="10,10,0,0" />
                                                <RadioButton
                                                      x:Name="Borg0"
                                                      VerticalAlignment="Top"
                                                      Margin="0,10,0,0"
                                                      GroupName="BlameRadio0"
                                                      Foreground="YellowGreen"
                                                      Content="Borg"
                                                      Tag="8"
                                                      Checked="OnBlameButtonsZeroClick"/>
                                            </StackPanel>
                                        </StackPanel>
                                    </ScrollViewer>
                                    <Border Grid.Row="3"
                                          Grid.Column="0"
                                          Grid.ColumnSpan="16"
                                          BorderThickness="2"
                                          Margin="10,0,0,5"
                                          Background="Black"
                                          BorderBrush="Red" />
                                    <!-- Covert Ops buttons Zero -->
                                    <StackPanel Grid.Row="3"
                                            Grid.Column="0"
                                            Orientation="Horizontal"
                                            HorizontalAlignment="Left"
                                            Margin="10,0,0,0">
                                        <TextBlock
                                             VerticalAlignment="Center"
                                             HorizontalAlignment="Left"
                                             Margin="10,0,0,0"
                                             FontSize="16"
                                             Foreground="AntiqueWhite"
                                             Text="2) Target: " />
                                        <TextBlock VerticalAlignment="Center"
                                               Text="{Binding Path=SpiedFedName}"/>
                                        <!--<Image Source="vfs:///Resources/Images/Insignias/FEDERATION.png" Margin="5,0,0,0">
                                        </Image>-->
                                        <!--Food Zero-->
                                        <scc:GameLabel
                                                   VerticalAlignment="Center"
                                                   Margin="14,0,2,0"
                                                   ImageSourceSmall="vfs:///Resources/Images/Resources/food.png"
                                                   ToolTip="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=FOOD_TOOL_TIP}" />
                                        <TextBlock
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Right"
                                                FontWeight="DemiBold"
                                                Foreground="LightGreen"
                                                Text="Food: " />
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*" />
                                                <RowDefinition Height="24" />
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                                   Grid.Row="1"
                                                   Margin="0,4,0,4"
                                                   VerticalAlignment="Center"
                                                   HorizontalAlignment="Left" />
                                            <scc:GameButton x:Name="SabotageFoodZero"
                                                      Grid.Row="1"
                                                      DisplayMode="Tiny"
                                                      VerticalAlignment="Center"
                                                      HorizontalAlignment="Left"
                                                      FontWeight="DemiBold"
                                                      Label="Sabotage Food"
                                                      Visibility="Collapsed"
                                                      Foreground="Black"
                                                      Background="Transparent"
                                                      BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                                      BorderThickness="2"
                                                      Click = "OnFoodZeroClick"/>
                                        </Grid>
                                        <!--Industry Zero-->
                                        <scc:GameLabel
                                                    VerticalAlignment="Center"
                                                    Margin="14,0,2,0"
                                                    ImageSourceSmall="vfs:///Resources/Images/ScreenIcons/industry.png"
                                                    ToolTip="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=INDUSTRY_TOOL_TIP}" />
                                        <TextBlock
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Right"
                                                FontWeight="DemiBold"
                                                Foreground="Orange"
                                                Text="Industry: " />
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*" />
                                                <RowDefinition Height="24" />
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                             Grid.Row="1"
                                             Margin="0,4,0,4"
                                             VerticalAlignment="Center"
                                             HorizontalAlignment="Left" />
                                            <scc:GameButton
                                                  x:Name="SabotageIndustryZero"
                                                  Grid.Row="1"
                                                  DisplayMode="Tiny"
                                                  VerticalAlignment="Center"
                                                  HorizontalAlignment="Left"
                                                  Label="Sabotage Industry"
                                                  Visibility="Collapsed"
                                                  FontWeight="DemiBold"
                                                  Foreground="Black"
                                                  Background="Transparent"
                                                  BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                                  Click = "OnIndustryZeroClick" />
                                        </Grid>
                                        <!--Energy Zero-->
                                        <scc:GameLabel
                                                    VerticalAlignment="Center"
                                                    Margin="14,0,2,0"
                                                    ImageSourceSmall="vfs:///Resources/Images/ScreenIcons/energy.png"
                                                    ToolTip="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=ENERGY_TOOL_TIP}" />
                                        <TextBlock
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Right"
                                                FontWeight="DemiBold"
                                                Foreground="Yellow"
                                                Text="Energy: " />
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*" />
                                                <RowDefinition Height="24" />
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                                   Grid.Row="1"
                                                   Margin="0,4,0,4"
                                                   VerticalAlignment="Center"
                                                   HorizontalAlignment="Left" />
                                            <scc:GameButton
                                                  x:Name="SabotageEnergyZero"
                                                  Grid.Row="1"
                                                  DisplayMode="Tiny"
                                                  VerticalAlignment="Center"
                                                  HorizontalAlignment="Left"
                                                  Label="Sabotage Energy"
                                                  Visibility="Collapsed"
                                                  FontWeight="DemiBold"
                                                  Foreground="Black"
                                                  Background="Transparent"
                                                  BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                                  Margin="0,0,5,0"
                                                  Click = "OnEnergyZeroClick" />
                                        </Grid>
                                        <!--Research Zero-->
                                        <scc:GameLabel
                                               VerticalAlignment="Center"
                                               Margin="14,0,2,0"
                                               ImageSourceSmall="vfs:///Resources/Images/ScreenIcons/research.png"
                                               ToolTip="{sx:LocalizedString Group={x:Static s:LocalizedTextGroups.AssetsScreen}, Entry=RESEARCH_TOOL_TIP}" />
                                        <TextBlock
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Right"
                                                FontWeight="DemiBold"
                                                Foreground="LightBlue"
                                                Text="Research: " />
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*" />
                                                <RowDefinition Height="24" />
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                                   Grid.Row="1"
                                                   Margin="0,4,0,4"
                                                   VerticalAlignment="Center"
                                                   HorizontalAlignment="Left" />
                                            <scc:GameButton
                                                  x:Name="StealResearchZero"
                                                  Grid.Row="1"
                                                  DisplayMode="Tiny"
                                                  VerticalAlignment="Center"
                                                  HorizontalAlignment="Left"
                                                  Label="Steal Research"
                                                  Visibility="Collapsed"
                                                  FontWeight="DemiBold"
                                                  Foreground="Black"
                                                  Background="Transparent"
                                                  BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                                  BorderThickness="2"
                                                  Click = "OnResearchZeroClick"/>
                                        </Grid>
                                        <!--Credit One-->
                                        <scc:GameLabel
                                                  VerticalAlignment="Center"
                                                  Margin="14,0,2,0"
                                                  ImageSourceSmall="vfs:///Resources/Images/ScreenIcons/credit.png" />
                                        <TextBlock
                                            VerticalAlignment="Center"
                                            HorizontalAlignment="Right"
                                            FontWeight="DemiBold"
                                            Foreground="LightBlue"
                                            Text="Credits: " />
                                        <Grid>
                                            <Grid.RowDefinitions>
                                                <RowDefinition Height="*" />
                                                <RowDefinition Height="24" />
                                                <RowDefinition Height="*" />
                                            </Grid.RowDefinitions>
                                            <Image Source="vfs:///Resources/Images/ScreenIcons/NotAllowed.png"
                                                     Grid.Row="1"
                                                     Margin="0,4,0,4"
                                                     VerticalAlignment="Center"
                                                     HorizontalAlignment="Left" />
                                            <scc:GameButton
                                                x:Name="StealCreditsZero"
                                                Grid.Row="1"
                                                DisplayMode="Tiny"
                                                VerticalAlignment="Center"
                                                HorizontalAlignment="Left"
                                                Label="Steal Credits"
                                                Visibility="Collapsed"
                                                FontWeight="DemiBold"
                                                Foreground="Black"
                                                Background="Transparent"
                                                BorderBrush="{DynamicResource {x:Static scc:ClientResources.DisabledTextForegroundBrushKey}}"
                                                Click = "OnCreditsZeroClick"/>
                                        </Grid>
                                    </StackPanel>
                                    <!-- Colonies - Zero -->
                                    <StackPanel Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="17" HorizontalAlignment="Left">
                                        <v:SpiedZeroColonyList DataContext="{Binding Path=SpiedZeroColonies}" Margin="10,0,0,0" />
                                    </StackPanel>
                                </Grid>
                            </Grid>
                        </Expander>
                        <TextBlock Height="5"/>
                        <!-- One Expander -->
                        <Expander x:Name="EmpireExpanderOne" Background="Transparent" FlowDirection="LeftToRight" Style="{StaticResource changeColorOnExpanded}"
                                 IsExpanded="True" Header="{Binding Path=SpiedOneCivName}" FontSize="20" BorderThickness="3" BorderBrush="White"
                                  Foreground="LightYellow" VerticalAlignment="Top" HorizontalAlignment="Left" Grid.ColumnSpan="4" Grid.Row="3">
                            <Grid Name="EmpireGridOne" FlowDirection="LeftToRight" Margin="1,1,5,5" HorizontalAlignment="Center">
                                <Grid FlowDirection="LeftToRight">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="45" />
                                        <RowDefinition Height="5" />
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="45" />
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <!--Blame Buttons One -->
                                    <Border Grid.Row="0"
                                            Grid.Column="0"
                                            Grid.ColumnSpan="18"
                                            BorderThickness="3"
                                            Margin="10,0,0,0"
                                            Background="Black"
                                            BorderBrush="DarkOrange" />
                                    <ScrollViewer
                                              HorizontalScrollBarVisibility="Auto"
                                              VerticalScrollBarVisibility="Disabled"
                                              HorizontalAlignment="Left"
                                              Grid.Row="0"
                                              Grid.ColumnSpan="18"
                                              VerticalAlignment="Top"
                                              Margin="0,5,0,0"
                                              Width="1000">
                                        <StackPanel x:Name="Blame1"
                                                  Orientation="Horizontal"
                                                  VerticalAlignment="Top"
